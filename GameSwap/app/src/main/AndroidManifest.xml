<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cmput301t4.gameswap" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".clickInventory"
            android:label="@string/title_activity_click_inventory" >
        </activity>
        <activity
            android:name=".myInventory"
            android:label="@string/title_activity_my_inventory" >
        </activity>
        <activity
            android:name=".selectTask"
            android:label="@string/title_activity_select_task" >
        </activity>
        <activity
            android:name=".selectTaskActivity"
            android:label="@string/title_activity_select_task" >
        </activity>
        <activity
            android:name=".myInventoryActivity"
            android:label="@string/title_activity_my_inventory" >
        </activity>
    </application>

</manifest>
